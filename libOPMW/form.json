{
    "begin": "opmw:WorkflowTemplate",
    "elements": {
        "opmw:WorkflowTemplate": {
            "properties": {
                "rdfs:label": {
                    "range": null,
                    "input": "text",
                    "dimension": 1,
                    "required": true,
                    "info": "name of experiment",
                    "label": "label"
                },
                "dcterms:contributor": {
                    "range": null,
                    "input": "text",
                    "dimension": "multi",
                    "required": true,
                    "info": "experiment contributor / author",
                    "label": "authors"
                },
                "opmw:hasDocumentation": {
                    "range": null,
                    "input": "textarea",
                    "dimension": 1,
                    "required": true,
                    "info": "has Documentation",
                    "label": "documentation"
                },
                "opmw:hasTemplateDiagram": {
                    "range": "URI",
                    "input": "text",
                    "dimension": 1,
                    "required": false,
                    "info": "has Template Diagram",
                    "label": "diagram"
                },
                "opmw:createdInWorkflowSystem": {
                    "range": "URI",
                    "input": "text",
                    "dimension": 1,
                    "required": false,
                    "info": "created in Workflow System",
                    "label": "workflow system"
                },
                "opmw:hasNativeSystemTemplate": {
                    "range": "URI",
                    "input": "text",
                    "dimension": 1,
                    "required": false,
                    "info": "has Native System Template",
                    "label": "native template"
                }
            },
            "relations": {
                "opmw:isParameterOfTemplate": {
                    "domain": "opmw:ParameterVariable",
                    "info": "is Parameter of Template",
                    "label": "parameters"
                },
                "opmw:isStepOfTemplate": {
                    "domain": "opmw:WorkflowTemplateProcess",
                    "info": "is Step of Template",
                    "label": "steps"
                },
                "opmw:correspondsToTemplate": {
                    "domain": "opmw:WorkflowExecutionAccount",
                    "info": "corresponds to Template",
                    "label": "runs"
                },
                "opmw:isVariableOfTemplate": {
                    "domain": "opmw:DataVariable",
                    "info": "is Variable of Template",
                    "label": "data variables"
                }
            },
            "label": "Experiment Template"
        },
        "opmw:WorkflowTemplateProcess": {
            "properties": {
                "rdfs:label": {
                    "range": null,
                    "input": "text",
                    "dimension": 1,
                    "required": true,
                    "info": "label for step",
                    "label": "label"
                },
                "opmw:uses": {
                    "range": [
                        "opmw:WorkflowTemplateArtifact",
                        "opmw:DataVariable",
                        "opmw:ParameterVariable"
                    ],
                    "input": "select",
                    "dimension": "multi",
                    "required": false,
                    "info": "variables used",
                    "label": "uses variables"
                }
            },
            "relations": {
                "opmw:isGeneratedBy": {
                    "domain": "opmw:WorkflowTemplateArtifact",
                    "info": "is Generated By",
                    "label": "generated by"
                },
                "opmw:correspondsToTemplateProcess": {
                    "domain": "opmw:WorkflowExecutionProcess",
                    "info": "runs",
                    "label": "execution steps"
                }
            },
            "label": "Step"
        },
        "opmw:DataVariable": {
            "properties": {
                "rdfs:label": {
                    "range": null,
                    "input": "text",
                    "dimension": 1,
                    "required": true,
                    "info": "label for step",
                    "label": "label"
                },
                "opmw:hasDimensionality": {
                    "range": "int",
                    "input": "text",
                    "dimension": 1,
                    "required": true,
                    "info": "has Dimensionality",
                    "label": "dimension"
                },
                "opmw:isGeneratedBy": {
                    "range": "opmw:WorkflowTemplateProcess",
                    "input": "select",
                    "dimension": 1,
                    "required": false,
                    "info": "is Generated By",
                    "label": "generated by"
                }
            },
            "relations": {
                "opmw:isUsesOf": {
                    "domain": "opmw:WorkflowTemplateProcess",
                    "info": "is used by"
                },
                "opmw:correspondsToTemplateArtifact": {
                    "domain": "opmw:WorkflowExecutionArtifact",
                    "info": "runtime data variables"
                }
            },
            "label": "Data Variable"
        },
        "opmw:ParameterVariable": {
            "properties": {
                "rdfs:label": {
                    "range": null,
                    "input": "text",
                    "dimension": 1,
                    "required": true,
                    "info": "label for step",
                    "label": "label"
                },
                "opmw:hasDimensionality": {
                    "range": "int",
                    "input": "text",
                    "dimension": 1,
                    "required": true,
                    "info": "has Dimensionality",
                    "label": "dimension"
                },
                "opmw:isGeneratedBy": {
                    "range": "opmw:WorkflowTemplateProcess",
                    "input": "select",
                    "dimension": 1,
                    "required": false,
                    "info": "is Generated By",
                    "label": "generated by"
                }
            },
            "relations": {
                "opmw:isUsesOf": {
                    "domain": "opmw:WorkflowTemplateProcess",
                    "info": "is used by"
                },
                "opmw:correspondsToTemplateArtifact": {
                    "domain": "opmw:WorkflowExecutionArtifact",
                    "info": "runtime parameters"
                }
            },
            "label": "Parameter"
        }
    },
    "execution": {
        "opmw:WorkflowExecutionAccount": {
            "properties": {
                "opmw:correspondsToTemplate": {
                    "range": "opmw:WorkflowTemplate",
                    "input": "select",
                    "info": "corresponds to Template"
                },
                "opmw:hasStatus": {
                    "range": "string",
                    "input": "text",
                    "info": "has Status"
                },
                "opmw:hasExecutionDiagram": {
                    "range": "URI",
                    "input": "text",
                    "info": "has Execution Diagram"
                },
                "opmw:executedInWorkflowSystem": {
                    "range": "ns:Agent",
                    "input": "select",
                    "info": "executed in Workflow System"
                },
                "opmw:overallStartTime": {
                    "range": "datetime",
                    "input": "text",
                    "info": "overall Start Time"
                },
                "opmw:overallEndTime": {
                    "range": "datetime",
                    "input": "text",
                    "info": "overall End Time"
                }
            },
            "relations": {},
            "label": "Run"
        },
        "opmw:WorkflowExecutionProcess": {
            "properties": {
                "opmw:hasExecutableComponent": {
                    "range": null,
                    "input": null,
                    "info": "has Executable Component"
                },
                "opmw:correspondsToTemplateProcess": {
                    "range": "opmw:WorkflowTemplateProcess",
                    "input": "select",
                    "info": "corresponds to Template Process"
                }
            },
            "relations": {},
            "label": "WorkflowExecutionProcess"
        },
        "opmw:WorkflowExecutionArtifact": {
            "properties": {
                "opmw:hasFileName": {
                    "range": "string",
                    "input": "text",
                    "info": "has File Name"
                },
                "opmw:hasValue": {
                    "range": "string",
                    "input": "text",
                    "info": "has Value"
                },
                "opmw:correspondsToTemplateArtifact": {
                    "range": "opmw:WorkflowTemplateArtifact",
                    "input": "select",
                    "info": "corresponds to Template Artifact"
                },
                "opmw:hasLocation": {
                    "range": "URI",
                    "input": "text",
                    "info": "has Location"
                },
                "opmw:hasSize": {
                    "range": "int",
                    "input": "text",
                    "info": "has Size"
                }
            },
            "relations": {},
            "label": "WorkflowExecutionArtifact"
        }
    }
}